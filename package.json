{
  "name": "@multiversx/sdk-event-processor",
  "version": "1.0.2",
  "description": "MultiversX logs&events processor",
  "main": "lib/event.processor.js",
  "types": "lib/event.processor.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "jest --testPathPattern='(?<!\\.e2e)\\.spec\\.ts$'",
    "test:e2e": "jest --testPathPattern='\\.e2e\\.spec\\.ts$'",
    "build": "tsc",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/multiversx/mx-sdk-event-processor.git"
  },
  "keywords": [
    "multiversx",
    "event",
    "processor"
  ],
  "author": "MultiversX",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/multiversx/mx-sdk-event-processor/issues"
  },
  "homepage": "https://github.com/multiversx/mx-sdk-event-processor#readme",
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "eslint": "^8.9.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "axios": "^1.7.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage/unit",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  }
}
